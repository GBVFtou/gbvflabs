{
  "name": "@gbvflabs/cli",
  "version": "0.1.7",
  "private": false,
  "bin": {
    "gbvflabs": "dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "node --watch src/index.ts",
    "build": "tsup",
    "release:beta": "pnpm build && pnpm version prerelease --preid=beta && pnpm publish --tag beta",
    "release:patch": "pnpm build && pnpm version patch && pnpm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18"
  }
}
